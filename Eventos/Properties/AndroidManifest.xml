	<permission android:name="dynamikcreative.eventos.permission.MAPS_RECEIVE" android:protectionLevel="signature" />
	<application android:label="Eventos">
		<!-- Put your Google Maps V2 API Key here. -->
		<meta-data android:name="com.google.android.maps.v2.API_KEY" android:value="AIzaSyDN2CMXudlsG6KugoFECae_mWuShBSqS6c" />
		<meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version" />
	</application>
</manifest>